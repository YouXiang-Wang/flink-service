{
  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "An address similar to http://microformats.org/wiki/h-card",
  "type": "object",
  "properties": {
    "key": {
      "type": "string"
    },
    "increment": {
      "type": "integer"
    },
    "id": {
      "type": "string"
    }
  },
  "required": [ "key", "increment", "id" ]
}
